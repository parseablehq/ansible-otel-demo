# Service identity for Resource
OTEL_SERVICE_NAME=ansible-playbook-demo

# Parseable HTTP ingest endpoint (example for local instance)
# Typically: http://localhost:8000/api/v1/ingest
PARSEABLE_ENDPOINT=http://localhost:8000/api/v1/ingest

# Base64("username:password") for Parseable basic auth
PARSEABLE_BASIC_AUTH_B64=REPLACE_ME_BASE64

# Target streams in Parseable
PARSEABLE_LOGS_STREAM=otel-logs
PARSEABLE_TRACES_STREAM=otel-traces
PARSEABLE_METRICS_STREAM=otel-metrics
